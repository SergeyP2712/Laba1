    "### 3.4. Выбросы (Outliers)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"=== АНАЛИЗ ВЫБРОСОВ ===\")\n",
    "\n",
    "# Выбираем числовой признак для анализа выбросов\n",
    "analysis_column = 'age'\n",
    "if analysis_column in df.columns:\n",
    "    print(f\"Анализ выбросов для признака: {analysis_column}\")\n",
    "    \n",
    "    # Метод межквартильного размаха (IQR)\n",
    "    Q1 = df[analysis_column].quantile(0.25)\n",
    "    Q3 = df[analysis_column].quantile(0.75)\n",
    "    IQR = Q3 - Q1\n",
    "    \n",
    "    lower_bound = Q1 - 1.5 * IQR\n",
    "    upper_bound = Q3 + 1.5 * IQR\n",
    "    \n",
    "    print(f\"Q1 (25-й перцентиль): {Q1:.2f}\")\n",
    "    print(f\"Q3 (75-й перцентиль): {Q3:.2f}\")\n",
    "    print(f\"IQR: {IQR:.2f}\")\n",
    "    print(f\"Нижняя граница: {lower_bound:.2f}\")\n",
    "    print(f\"Верхняя граница: {upper_bound:.2f}\")\n",
    "    \n",
    "    outliers = df[(df[analysis_column] < lower_bound) | (df[analysis_column] > upper_bound)]\n",
    "    print(f\"\\nКоличество выбросов (метод IQR): {len(outliers)}\")\n",
    "    \n",
    "    if len(outliers) > 0:\n",
    "        print(\"Примеры выбросов:\")\n",
    "        print(outliers[['full_name', 'year', 'category', analysis_column]].head())\n",
    "    \n",
    "    # Визуализация boxplot\n",
    "    plt.figure(figsize=(10, 6))\n",
    "    plt.boxplot(df[analysis_column].dropna())\n",
    "    plt.title(f'Boxplot для признака \"{analysis_column}\"')\n",
    "    plt.ylabel('Значение')\n",
    "    plt.grid(True, alpha=0.3)\n",
    "    plt.show()\n",
    "    \n",
    "    # Scatter plot для визуализации выбросов\n",
    "    plt.figure(figsize=(12, 6))\n",
    "    plt.scatter(range(len(df)), df[analysis_column], alpha=0.6, label='Все значения')\n",
    "    plt.scatter(outliers.index, outliers[analysis_column], color='red', alpha=0.8, label='Выбросы')\n",
    "    plt.axhline(y=upper_bound, color='r', linestyle='--', alpha=0.5, label='Верхняя граница')\n",
    "    plt.axhline(y=lower_bound, color='r', linestyle='--', alpha=0.5, label='Нижняя граница')\n",
    "    plt.title(f'Выбросы в признаке \"{analysis_column}\"')\n",
    "    plt.xlabel('Индекс записи')\n",
    "    plt.ylabel(analysis_column)\n",
    "    plt.legend()\n",
    "    plt.grid(True, alpha=0.3)\n",
    "    plt.show()"
   ]
