for col in df.columns:
    dtype = str(df[col].dtype)
    
    # Предположительный смысл на основе названия
    meanings = {
        'year': 'Год присуждения премии',
        'category': 'Категория премии (физика, химия и т.д.)',
        'prize': 'Полное название премии',
        'prize_share': 'Доля премии (если несколько лауреатов)',
        'laureate_id': 'Уникальный идентификатор лауреата',
        'laureate_type': 'Тип лауреата (индивидуальный/организация)',
        'full_name': 'Полное имя лауреата',
        'birth_date': 'Дата рождения',
        'birth_city': 'Город рождения',
        'birth_country': 'Страна рождения',
        'sex': 'Пол лауреата',
        'organization_name': 'Название организации',
        'organization_city': 'Город организации',
        'organization_country': 'Страна организации',
        'death_date': 'Дата смерти',
        'death_city': 'Город смерти',
        'death_country': 'Страна смерти',
        'usa_born_winner': 'Родился ли в США',
        'decade': 'Декада присуждения',
        'female': 'Является ли женщиной',
        'age': 'Возраст при получении премии',
        'age_group': 'Возрастная группа'
    }
    
    meaning = meanings.get(col, 'Неизвестно - требует уточнения')
    column_info.append([col, dtype, meaning])

# Создаём DataFrame для красивого отображения
col_df = pd.DataFrame(column_info, columns=['Признак', 'Тип данных', 'Предположительный смысл'])
print(col_df.to_string(index=False))
print("\n")

# %%
# 2.3. Предварительный просмотр данных
print("=== ПЕРВЫЕ 5 СТРОК ===")
print(df.head())
print("\n")

print("=== ОБЩАЯ ИНФОРМАЦИЯ ===")
print(df.info())
print("\n")

# %%
# 2.4. Приведение типов данных к более подходящим
print("=== ПРИВЕДЕНИЕ ТИПОВ ДАННЫХ ===")

# Преобразуем даты
date_columns = ['birth_date', 'death_date']
for col in date_columns:
    if col in df.columns:
        df[col] = pd.to_datetime(df[col], errors='coerce')
        print(f"Столбец '{col}' преобразован в datetime")

# Преобразуем логические колонки (если они в строковом формате)
bool_columns = ['usa_born_winner', 'female']
for col in bool_columns:
    if col in df.columns:
        # Проверяем, есть ли уже булевые значения
        if df[col].dtype == 'object':
            df[col] = df[col].replace({'True': True, 'False': False, 'true': True, 'false': False})
        print(f"Столбец '{col}' проверен на соответствие bool")

# Категориальные колонки
cat_columns = ['category', 'sex', 'age_group', 'laureate_type']
for col in cat_columns:
    if col in df.columns:
        df[col] = df[col].astype('category')
        print(f"Столбец '{col}' преобразован в категориальный")

print("\nОбновлённые типы данных:")
print(df.dtypes)

# %% [markdown]
# ## 3. Аудит качества данных

# %% [markdown]
# ### 3.1. Пропуски (Missing Values)

# %%
print("=== АНАЛИЗ ПРОПУЩЕННЫХ ЗНАЧЕНИЙ ===")

# Вычисляем количество и долю пропусков
missing_data = pd.DataFrame({
    'Колонка': df.columns,
    'Количество пропусков': df.isnull().sum(),
    'Доля пропусков (%)': (df.isnull().sum() / len(df) * 100).round(2)
})

# Сортируем по убыванию количества пропусков
missing_data = missing_data.sort_values('Доля пропусков (%)', ascending=False)

print("Все пропуски по колонкам:")
print(missing_data.to_string(index=False))
print("\n")

# Выделяем наиболее проблемные признаки
problematic = missing_data[missing_data['Доля пропусков (%)'] > 0]
if len(problematic) > 0:
    print("=== НАИБОЛЕЕ ПРОБЛЕМНЫЕ ПРИЗНАКИ ===")
    print(problematic.to_string(index=False))
    
    print("\n=== ВЫВОДЫ О ПРОПУСКАХ ===")
    print("1. Наиболее проблемные признаки с большим % пропусков:")
    top_problematic = problematic.head(3)
    for _, row in top_problematic.iterrows():
        print(f"   - {row['Колонка']}: {row['Доля пропусков (%)']}% пропусков")
    
    print("\n2. Критичность пропусков:")
    print("   - death_* колонки: не критичны для анализа лауреатов (не все умерли)")
    print("   - organization_*: могут быть важны для анализа институциональной принадлежности")
    print("   - age: КРИТИЧНЫ - влияют на анализ возрастного распределения")
else:
    print("Пропусков не обнаружено!")

# %% [markdown]
# ### 3.2. Дубликаты

# %%
print("=== АНАЛИЗ ДУБЛИКАТОВ ===")

# Полные дубликаты строк
full_duplicates = df.duplicated().sum()
print(f"Количество полностью дублирующихся строк: {full_duplicates}")

if full_duplicates > 0:
    print("Примеры дубликатов:")
    print(df[df.duplicated()].head())
else:
    print("Полных дубликатов не обнаружено.")

print("\n")

# Проверка дубликатов по ключевым полям
key_columns = ['laureate_id', 'year', 'category']
if all(col in df.columns for col in key_columns):
    key_duplicates = df.duplicated(subset=key_columns, keep=False).sum()
    print(f"Дубликаты по ключевым полям {key_columns}: {key_duplicates}")
    
    if key_duplicates > 0:
        duplicates = df[df.duplicated(subset=key_columns, keep=False)]
        print("Примеры дубликатов по ключевым полям:")
        print(duplicates.sort_values(key_columns).head(10))
    else:
        print("Дубликатов по ключевым полям не обнаружено.")

# %% [markdown]
# ### 3.3. Типические проблемы значений

# %%
print("=== ПРОВЕРКА ЧИСЛОВЫХ ПРИЗНАКОВ ===")

numeric_cols = df.select_dtypes(include=[np.number]).columns
print(f"Числовые признаки: {list(numeric_cols)}")

for col in numeric_cols:
    if col in df.columns:
        print(f"\n--- {col} ---")
        print(f"Минимум: {df[col].min():.2f}")
        print(f"Максимум: {df[col].max():.2f}")
        print(f"Среднее: {df[col].mean():.2f}")
        print(f"Стандартное отклонение: {df[col].std():.2f}")
        print(f"Медиана: {df[col].median():.2f}")
        
        # Проверка на невозможные значения
        if col == 'age':
            suspicious = df[(df[col] < 15) | (df[col] > 100)]
            if len(suspicious) > 0:
                print(f"ПОДОЗРИТЕЛЬНЫЕ ЗНАЧЕНИЯ (возраст <15 или >100): {len(suspicious)} записей")
                print(suspicious[[col, 'full_name', 'year']].head())
        elif col == 'year':
            suspicious = df[df[col] < 1900]
            if len(suspicious) > 0:
                print(f"ПОДОЗРИТЕЛЬНЫЕ ЗНАЧЕНИЯ (год <1900): {len(suspicious)}")

# %%
print("\n=== ПРОВЕРКА КАТЕГОРИАЛЬНЫХ ПРИЗНАКОВ ===")

categorical_cols = df.select_dtypes(include=['object', 'category']).columns
print(f"Категориальные признаки: {list(categorical_cols)}")

for col in categorical_cols:
    if col in df.columns and col not in ['prize']:  # Пропускаем длинный текст
        print(f"\n--- {col} ---")
        unique_count = df[col].nunique()
        print(f"Уникальных значений: {unique_count}")
        
        if unique_count < 20:  # Показываем только для колонок с малым числом уникальных значений
            print("Распределение:")
            print(df[col].value_counts().head(10))
        
        # Проверка на "грязные" категории
        if df[col].dtype == 'object':
            # Проверка на разные регистры
            lower_values = df[col].dropna().astype(str).str.lower()
            original_values = df[col].dropna().astype(str)
            if not lower_values.equals(original_values.str.lower()):
                print("ВНИМАНИЕ: Возможно разные регистры в данных")
            
            # Проверка на лишние пробелы
            stripped_values = df[col].dropna().astype(str).str.strip()
            if not stripped_values.equals(original_values.str.strip()):
                print("ВНИМАНИЕ: Возможны лишние пробелы в данных")

# %% [markdown]
# ### 3.4. Выбросы (Outliers)

# %%
print("=== АНАЛИЗ ВЫБРОСОВ ===")

# Выбираем числовой признак для анализа выбросов
analysis_column = 'age'
if analysis_column in df.columns:
    print(f"Анализ выбросов для признака: {analysis_column}")
    
    # Метод межквартильного размаха (IQR)
    Q1 = df[analysis_column].quantile(0.25)
    Q3 = df[analysis_column].quantile(0.75)
    IQR = Q3 - Q1
    
    lower_bound = Q1 - 1.5 * IQR
    upper_bound = Q3 + 1.5 * IQR
    
    print(f"Q1 (25-й перцентиль): {Q1:.2f}")
    print(f"Q3 (75-й перцентиль): {Q3:.2f}")
    print(f"IQR: {IQR:.2f}")
    print(f"Нижняя граница: {lower_bound:.2f}")
    print(f"Верхняя граница: {upper_bound:.2f}")
    
    outliers = df[(df[analysis_column] < lower_bound) | (df[analysis_column] > upper_bound)]
    print(f"\nКоличество выбросов (метод IQR): {len(outliers)}")
    
    if len(outliers) > 0:
        print("Примеры выбросов:")
        print(outliers[['full_name', 'year', 'category', analysis_column]].head())
    
    # Визуализация boxplot
    plt.figure(figsize=(10, 6))
    plt.boxplot(df[analysis_column].dropna())
    plt.title(f'Boxplot для признака "{analysis_column}"')
    plt.ylabel('Значение')
    plt.grid(True, alpha=0.3)
    plt.show()
    
    # Scatter plot для визуализации выбросов
    plt.figure(figsize=(12, 6))
    plt.scatter(range(len(df)), df[analysis_column], alpha=0.6, label='Все значения')
    plt.scatter(outliers.index, outliers[analysis_column], color='red', alpha=0.8, label='Выбросы')
    plt.axhline(y=upper_bound, color='r', linestyle='--', alpha=0.5, label='Верхняя граница')
    plt.axhline(y=lower_bound, color='r', linestyle='--', alpha=0.5, label='Нижняя граница')
    plt.title(f'Выбросы в признаке "{analysis_column}"')
    plt.xlabel('Индекс записи')
    plt.ylabel(analysis_column)
    plt.legend()
    plt.grid(True, alpha=0.3)
    plt.show()

